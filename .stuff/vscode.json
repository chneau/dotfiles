{
	"editor.codeActionsOnSave": {
		"quickfix.biome": "explicit",
		"source.organizeImports.biome": "explicit",
		"source.addMissingImports": "explicit"
	},
	"remote.SSH.defaultExtensions": [
		"biomejs.biome",
		"chneau.resource-monitor",
		"chneau.saturated-dark-modern",
		"dakara.transformer",
		"esbenp.prettier-vscode",
		"GitHub.copilot",
		"golang.go",
		"ms-dotnettools.csharp",
		"ms-vscode.vscode-typescript-next",
		"Prisma.prisma",
		"VisualStudioExptTeam.vscodeintellicode",
		"yoavbls.pretty-ts-errors"
	],
	"files.exclude": {
		"**/obj/": true,
		"**/bin/": true,
		"**/node_modules/": true,
		"**/.meteor/local/": true
	},
	"[aspnetcorerazor]": { "editor.formatOnSave": false },
	"[css]": { "editor.defaultFormatter": "biomejs.biome" },
	"[dotenv]": { "editor.formatOnSave": false },
	"[html]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
	"[javascript]": { "editor.defaultFormatter": "biomejs.biome" },
	"[javascriptreact]": { "editor.defaultFormatter": "biomejs.biome" },
	"[json]": { "editor.defaultFormatter": "biomejs.biome" },
	"[jsonc]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
	"[less]": { "editor.defaultFormatter": "biomejs.biome" },
	"[markdown]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
	"[razor]": { "editor.formatOnSave": false },
	"[scss]": { "editor.defaultFormatter": "biomejs.biome" },
	"[typescript]": { "editor.defaultFormatter": "biomejs.biome" },
	"[typescriptreact]": { "editor.defaultFormatter": "biomejs.biome" },
	"[yaml]": { "editor.defaultFormatter": "biomejs.biome" },
	"dotnet.completion.showCompletionItemsFromUnimportedNamespaces": true,
	"editor.accessibilitySupport": "off",
	"editor.bracketPairColorization.enabled": true,
	"editor.formatOnSave": true,
	"editor.guides.bracketPairs": true,
	"editor.hover.delay": 100,
	"editor.inlineSuggest.enabled": true,
	"editor.renderControlCharacters": true,
	"editor.renderWhitespace": "boundary",
	"editor.semanticHighlighting.enabled": true,
	"editor.stickyScroll.enabled": false,
	"editor.suggest.preview": true,
	"editor.suggestSelection": "first",
	"editor.tabSize": 2,
	"editor.wordWrap": "on",
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"extensions.ignoreRecommendations": true,
	"git.autofetch": true,
	"git.confirmSync": false,
	"git.enableSmartCommit": true,
	"git.openRepositoryInParentFolders": "always",
	"git.postCommitCommand": "sync",
	"github.copilot.enable": { "*": true },
	"notebook.formatOnSave.enabled": true,
	"remote.SSH.remotePlatform": { "ncce": "linux" },
	"security.workspace.trust.enabled": false,
	"telemetry.telemetryLevel": "off",
	"terminal.integrated.allowChords": false,
	"terminal.integrated.showExitAlert": false,
	"typescript.preferences.useAliasesForRenames": false,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"window.commandCenter": false,
	"window.zoomLevel": -1,
	"workbench.colorTheme": "Saturated Dark Modern",
	"workbench.tree.renderIndentGuides": "always",
	"terminal.integrated.enableMultiLinePasteWarning": "never",
	"workbench.activityBar.location": "top"
}
