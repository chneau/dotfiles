{
	"[aspnetcorerazor]": { "editor.formatOnSave": false },
	"[css]": { "editor.defaultFormatter": "biomejs.biome" },
	"[dotenv]": { "editor.formatOnSave": false },
	"[html]": { "editor.defaultFormatter": "biomejs.biome" },
	"[javascript]": { "editor.defaultFormatter": "biomejs.biome" },
	"[javascriptreact]": { "editor.defaultFormatter": "biomejs.biome" },
	"[json]": { "editor.defaultFormatter": "biomejs.biome" },
	"[jsonc]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
	"[less]": { "editor.defaultFormatter": "biomejs.biome" },
	"[markdown]": {
		"files.trimTrailingWhitespace": false,
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[razor]": { "editor.formatOnSave": false },
	"[scss]": { "editor.defaultFormatter": "biomejs.biome" },
	"[typescript]": { "editor.defaultFormatter": "biomejs.biome" },
	"[typescriptreact]": { "editor.defaultFormatter": "biomejs.biome" },
	"[yaml]": { "editor.defaultFormatter": "biomejs.biome" },
	"editor.tabSize": 2,
	"debug.onTaskErrors": "debugAnyway",
	"dotnet.completion.showCompletionItemsFromUnimportedNamespaces": true,
	"editor.acceptSuggestionOnCommitCharacter": false,
	"editor.accessibilitySupport": "off",
	"editor.bracketPairColorization.enabled": true,
	"editor.codeActionsOnSave": {
		"quickfix.biome": "explicit",
		"source.organizeImports.biome": "explicit",
		"source.addMissingImports": "explicit"
	},
	"editor.detectIndentation": true,
	"editor.formatOnSave": true,
	"editor.guides.bracketPairs": true,
	"editor.hover.delay": 100,
	"editor.inlineSuggest.enabled": true,
	"editor.linkedEditing": true,
	"editor.minimap.showSlider": "always",
	"editor.renderControlCharacters": true,
	"editor.renderWhitespace": "boundary",
	"editor.semanticHighlighting.enabled": true,
	"editor.suggest.preview": true,
	"editor.suggestSelection": "first",
	"editor.wordWrap": "on",
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"extensions.ignoreRecommendations": true,
	"files.exclude": {
		"**/*.cshtml__virtual.html": true,
		"**/obj/": true,
		"**/bin/": true,
		"**/*.razor__bg__virtual.cs": true,
		"**/node_modules/": true,
		"**/.meteor/local/": true,
		"**/__pycache__/": true,
		"**/zig-cache/**": true,
		"**/zig-out/**": true
	},
	"files.insertFinalNewline": true,
	"files.trimFinalNewlines": true,
	"files.trimTrailingWhitespace": true,
	"files.watcherExclude": {
		"**/.git/objects/**": true,
		"**/.git/subtree-cache/**": true,
		"**/node_modules/*/**": true,
		"**/.hg/store/**": true,
		"**/*.cshtml__virtual.html": true,
		"**/obj/**": true,
		"**/bin/**": true,
		"**/*.razor__bg__virtual.cs": true,
		"**/node_modules/**": true,
		"**/.meteor/local/**": true,
		"**/zig-cache/**": true,
		"**/zig-out/**": true
	},
	"git.autofetch": true,
	"git.confirmSync": false,
	"git.enableSmartCommit": true,
	"git.openRepositoryInParentFolders": "always",
	"git.postCommitCommand": "sync",
	"git.pruneOnFetch": true,
	"git.rebaseWhenSync": true,
	"github.copilot.enable": {
		"*": true,
		"plaintext": false,
		"markdown": true,
		"scminput": false
	},
	"go.coverageDecorator": { "type": "gutter" },
	"go.coverOnSave": true,
	"go.coverOnSingleTest": true,
	"go.lintTool": "golangci-lint",
	"go.toolsManagement.autoUpdate": true,
	"go.useLanguageServer": true,
	"javascript.preferences.useAliasesForRenames": false,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"notebook.formatOnSave.enabled": true,
	"omnisharp.useModernNet": true,
	"remote.SSH.defaultExtensions": [
		"biomejs.biome",
		"chneau.resource-monitor",
		"chneau.saturated-dark-plus",
		"chneau.saturated-dark-modern",
		"dakara.transformer",
		"dsznajder.es7-react-js-snippets",
		"biomejs.biome",
		"GitHub.copilot",
		"golang.go",
		"hashicorp.terraform",
		"jebbs.plantuml",
		"mechatroner.rainbow-csv",
		"ms-dotnettools.csharp",
		"ms-vscode-remote.remote-ssh",
		"ms-vscode.js-debug-nightly",
		"ms-vscode.remote-explorer",
		"ms-vscode.vscode-typescript-next",
		"styled-components.vscode-styled-components",
		"timonwong.shellcheck",
		"VisualStudioExptTeam.vscodeintellicode"
	],
	"remote.SSH.remotePlatform": { "nc": "linux" },
	"search.exclude": {
		"**/node_modules": true,
		"**/bower_components": true,
		"**/*.code-search": true,
		"**/package-lock.json": true
	},
	"search.quickOpen.includeSymbols": true,
	"security.workspace.trust.enabled": false,
	"telemetry.telemetryLevel": "off",
	"terminal.integrated.allowChords": false,
	"terminal.integrated.defaultProfile.windows": "Git Bash",
	"terminal.integrated.scrollback": 10000,
	"terminal.integrated.showExitAlert": false,
	"typescript.preferences.useAliasesForRenames": false,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"window.commandCenter": false,
	"workbench.colorTheme": "Saturated Dark Modern",
	"workbench.editor.decorations.badges": true,
	"workbench.editor.decorations.colors": true,
	"autopep8.args": ["--ignore=E501"],
	"workbench.editor.highlightModifiedTabs": true,
	"workbench.editorAssociations": { "*.ipynb": "jupyter-notebook" },
	"workbench.layoutControl.enabled": false,
	"workbench.tree.renderIndentGuides": "always",
	"git.ignoreRebaseWarning": true,
	"zig.initialSetupDone": true,
	"diffEditor.ignoreTrimWhitespace": false,
	"editor.stickyScroll.enabled": false
}
