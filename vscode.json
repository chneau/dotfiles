{
  "[aspnetcorerazor]": { "editor.formatOnSave": false },
  "[html]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[javascript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[json]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[jsonc]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[markdown]": { "files.trimTrailingWhitespace": false, "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[razor]": { "editor.formatOnSave": false },
  "[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[typescriptreact]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[yaml]": { "editor.defaultFormatter": "redhat.vscode-yaml" },
  "csharp.semanticHighlighting.enabled": true,
  "csharp.showOmnisharpLogOnError": false,
  "csharp.suppressBuildAssetsNotification": true,
  "customizeUI.stylesheet": { ".quick-input-widget": "width: 90% !important; left: calc(10% + 200px);" },
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.accessibilitySupport": "off",
  "editor.codeActionsOnSave": { "source.organizeImports": true },
  "editor.detectIndentation": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.hover.delay": 100,
  "editor.linkedEditing": true,
  "editor.minimap.showSlider": "always",
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "boundary",
  "editor.semanticHighlighting.enabled": true,
  "editor.suggest.preview": true,
  "editor.suggestSelection": "first",
  "editor.wordWrap": "on",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": true,
  "files.autoSave": "onWindowChange",
  "files.exclude": { "**/*.cshtml__virtual.html": true, "**/obj/": true, "**/bin/": true, "**/*.razor__bg__virtual.cs": true, "**/node_modules/": true },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.promptToSaveFilesBeforeCommit": "always",
  "go.gopath": "~/go",
  "go.lintTool": "golangci-lint",
  "go.toolsManagement.autoUpdate": true,
  "go.useLanguageServer": true,
  "java.configuration.checkProjectSettingsExclusions": false,
  "java.project.importOnFirstTimeStartup": "automatic",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "omnisharp.disableMSBuildDiagnosticWarning": true,
  "omnisharp.enableImportCompletion": true,
  "omnisharp.enableRoslynAnalyzers": true,
  "prettier.printWidth": 99999,
  "python.formatting.autopep8Args": ["--max-line-length=200"],
  "python.languageServer": "Pylance",
  "razor.format.enable": false,
  "search.quickOpen.includeSymbols": true,
  "security.workspace.trust.enabled": false,
  "telemetry.enableTelemetry": false,
  "terminal.external.windowsExec": "C:\\Program Files\\Git\\git-bash.exe",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.scrollback": 10000,
  "terminal.integrated.showExitAlert": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.zoomLevel": -1,
  "workbench.colorTheme": "Saturated Dark+",
  "workbench.editor.decorations.badges": true,
  "workbench.editor.decorations.colors": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editorAssociations": [{ "viewType": "jupyter.notebook.ipynb", "filenamePattern": "*.ipynb" }],
  "workbench.tree.renderIndentGuides": "always"
}
